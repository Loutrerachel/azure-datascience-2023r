{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-upds-rshs-dev-u4p"
		},
		"ADLSLinkedService_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLSLinkedService'"
		},
		"FileLinkedService_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'FileLinkedService'"
		},
		"FileServerTemporal_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'FileServerTemporal'"
		},
		"ADLSLinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlsupdsrshsdev5kf.dfs.core.windows.net/"
		},
		"FileLinkedService_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "\\\\HOME\\D$\\data"
		},
		"FileLinkedService_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "Raquel"
		},
		"FileServerTemporal_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "\\\\HOME\\D$\\data"
		},
		"FileServerTemporal_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "Raquel"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-upds-rshs-dev-u4p_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLSLinkedService_accountKey": {
						"value": "[parameters('ADLSLinkedService_accountKey')]"
					},
					"FileLinkedService_password": {
						"value": "[parameters('FileLinkedService_password')]"
					},
					"FileServerTemporal_password": {
						"value": "[parameters('FileServerTemporal_password')]"
					},
					"ADLSLinkedService_properties_typeProperties_url": {
						"value": "[parameters('ADLSLinkedService_properties_typeProperties_url')]"
					},
					"FileLinkedService_properties_typeProperties_host": {
						"value": "[parameters('FileLinkedService_properties_typeProperties_host')]"
					},
					"FileLinkedService_properties_typeProperties_userId": {
						"value": "[parameters('FileLinkedService_properties_typeProperties_userId')]"
					},
					"FileServerTemporal_properties_typeProperties_host": {
						"value": "[parameters('FileServerTemporal_properties_typeProperties_host')]"
					},
					"FileServerTemporal_properties_typeProperties_userId": {
						"value": "[parameters('FileServerTemporal_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}